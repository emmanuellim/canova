{
	"AWSTemplateFormatVersion" : "2010-09-09",
	
	"Description" : "AWS CloudFormation Template for MAEStream: MAEStream is a revenue protection solution for utilities supply and distribution bodies. This template is for municipality level deployment.",
	
    "Resources" : {
        "MAESGeolocIngRichmondHill" : {
            "Type" : "AWS::S3::Bucket",
            "Properties" : {
			"BucketName" 	: "maes.geoloc.ing.richmond-hill",
			"AccessControl" : "BucketOwnerFullControl"
			} 
		},
		"MAESAssetIngRichmondHill" : {
            "Type" : "AWS::S3::Bucket",
            "Properties" : {
			"BucketName" 	: "maes.asset.ing.richmond-hill",
			"AccessControl" : "BucketOwnerFullControl"
			}
        }
    }
}
